{
  "name": "@nest-openapi/serializer",
  "version": "0.1.0",
  "description": "Highâ€‘performance response serialization based on your OpenAPI specifications",
  "sideEffects": false,
  "keywords": [
    "nest",
    "nestjs",
    "openapi",
    "swagger",
    "serializer",
    "fast-json-stringify",
    "typescript",
    "jsonschema"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --target node20 --outDir dist --clean --external rxjs --external @nestjs/common --external @nestjs/core --external fast-json-stringify",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "homepage": "https://nest-openapi.github.io/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ts-oas/nest-openapi.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Alireza Malek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ts-oas/nest-openapi/issues"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.0 || ^10.0.0 || ^11.0.0",
    "@nestjs/core": "^9.0.0 || ^10.0.0 || ^11.0.0",
    "reflect-metadata": "^0.2.2"
  },
  "dependencies": {
    "@nest-openapi/runtime": "^0.1.1",
    "fast-json-stringify": "^6.0.1",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nest-openapi/runtime": "workspace:^",
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/testing": "^11.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "jest": "^29.0.0",
    "reflect-metadata": "^0.2.2",
    "supertest": "^7.1.0",
    "ts-jest": "^29.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": "test/.*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s",
      "!src/**/*.d.ts"
    ],
    "coverageDirectory": "coverage",
    "testEnvironment": "node"
  }
}
